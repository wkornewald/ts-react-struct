{
  "compilerOptions": {
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strict": true,
    "strictNullChecks": true,
    "lib": ["dom", "es6", "dom.iterable", "scripthost"],
    "module": "es2015",
    "target": "es5",
    "moduleResolution": "node",
    "jsx": "react",
    "outDir": "_generated_js",
    "baseUrl": "."
  }
}
