    "@types/react-dom": "^15.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "ts-immutable-struct": "^0.7.3"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.1",
    "@types/jest": "^19.2.3",
    "@types/react-test-renderer": "^15.5.1",
    "enzyme": "^2.9.1",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "ts-jest": "^20.0.5",
    "ts-node": "^3.0.4",
    "tslint": "^5.3.2",
    "typescript": "^2.3.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!ts-immutable-struct)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
