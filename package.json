{
  "name": "ts-react-struct",
  "version": "0.5.3",
  "author": {
    "name": "Waldemar Kornewald",
    "url": "https://github.com/wkornewald"
  },
  "keywords": [
    "cursor",
    "immutable",
    "immstruct",
    "om",
    "omniscient",
    "react",
    "redux",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wkornewald/ts-react-struct.git"
  },
  "main": "src",
